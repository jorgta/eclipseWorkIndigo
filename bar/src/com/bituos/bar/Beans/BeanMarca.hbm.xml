<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC

"-//Hibernate/Hibernate Mapping DTD//EN"

"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
   <class name="com.bituos.bar.Beans.BeanMarca" table="marca" >
       <id name="id" type="integer" column="id" unsaved-value="-1" >
          <generator class="increment"/>
       </id>
       <property name="marca" column="marca" type="string"  not-null="false"/>
       <property name="foto" column="foto" type="binary"  not-null="false"/>

   </class>
</hibernate-mapping>


