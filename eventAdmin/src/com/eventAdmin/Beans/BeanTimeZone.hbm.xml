<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC

"-//Hibernate/Hibernate Mapping DTD//EN"

"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
   <class name="com.eventAdmin.Beans.BeanTimeZone" table="timezone">
       <id name="id" type="integer" column="ID" unsaved-value="-1" >
          <generator class="increment"/>
       </id>
     
     <property name="timezoneOffset" column="timezoneOffset" type="integer"  not-null="true"/>
       <property name="displayLabel" column="displayLabel" type="string"  not-null="true"/>
       <property name="tooltip" column="tooltip" type="string"  not-null="true"/>
       
  
	 

   </class>
</hibernate-mapping>


