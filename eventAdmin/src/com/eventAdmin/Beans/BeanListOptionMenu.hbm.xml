<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC

"-//Hibernate/Hibernate Mapping DTD//EN"

"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
   <class name="com.eventAdmin.Beans.BeanListOptionMenu" table="list_option_menu">
       <id name="id" type="integer" column="ID" unsaved-value="-1" >
          <generator class="increment"/>
       </id>
       
       <many-to-one name="id_list_option" column="id_list_option" not-null="true"/>  
       <property name="description" column="description" type="string"  not-null="true"/>
       <property name="name" column="name" type="string"  not-null="true"/>
       <property name="active" column="active" type="string"  not-null="true"/>

      <property name="price1" column="price1" type="double"  not-null="true"/>
      <property name="price2" column="price2" type="double"  not-null="true"/>
      <property name="price3" column="price3" type="double"  not-null="true"/>
      <property name="price4" column="price4" type="double"  not-null="true"/>
      <property name="price5" column="price5" type="double"  not-null="true"/>
      <property name="price6" column="price6" type="double"  not-null="true"/>
      <property name="price7" column="price7" type="double"  not-null="true"/>
     
       
   </class>
</hibernate-mapping>


