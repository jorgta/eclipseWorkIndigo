<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC

"-//Hibernate/Hibernate Mapping DTD//EN"

"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
   <class name="com.struts2.Beans.BeanModule" table="modules" >
       <id name="id" type="integer" column="ID" unsaved-value="-1" >
          <generator class="increment"/>
       </id>
       <property name="title" column="title" type="string"  not-null="true"/>
       <property name="ordering" column="ordering" type="integer"  not-null="false"/>
       <property name="position" column="position" type="string"  not-null="true"/>
       <property name="module" column="module" type="string"  not-null="true"/>
       <property name="active" column="active" type="integer"  not-null="false"/>
       <property name="showtitle" column="showtitle" type="integer"  not-null="false"/>


       
   </class>
</hibernate-mapping>


