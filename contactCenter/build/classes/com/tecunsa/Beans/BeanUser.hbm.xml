<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC

"-//Hibernate/Hibernate Mapping DTD//EN"

"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
   <class name="com.tecunsa.Beans.BeanUser" table="user" >
       <id name="id" type="integer" column="ID" unsaved-value="-1" >
          <generator class="increment"/>
       </id>
       <property name="name" column="NAME" type="string"  not-null="true"/>
       <property name="full_name" column="FULL_NAME" type="string"  not-null="true"/>
       <property name="password" column="PASSWORD" type="string"  not-null="true"/>
       <property name="active" column="ACTIVE" type="string"  not-null="true"/>
       <property name="address" column="ADDRESS" type="string"  not-null="false"/>
       <property name="rfc" column="RFC" type="string"  not-null="false"/>
       <property name="email" column="EMAIL" type="string"  not-null="false"/>
       <property name="telephone" column="telephone" type="string"  not-null="false"/>
       
   </class>
</hibernate-mapping>


