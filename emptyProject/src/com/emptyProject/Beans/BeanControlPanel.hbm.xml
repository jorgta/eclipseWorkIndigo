<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC

"-//Hibernate/Hibernate Mapping DTD//EN"

"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
   <class name="com.bituos.lca.Beans.BeanControlPanel" table="control_panel">
       <id name="id" type="integer" column="ID" unsaved-value="-1" >
          <generator class="increment"/>
       </id>

       <property name="email_email" column="email_email" type="string"  not-null="true"/>
       <property name="email_username" column="email_username" type="string"  not-null="true"/>
       <property name="email_password" column="email_password" type="string"  not-null="true"/>
       <property name="email_smtp_server" column="email_smtp_server" type="string"  not-null="true"/>
       <property name="email_smtp_port" column="email_smtp_port" type="string"  not-null="true"/>
       <property name="email_is_secure" column="email_is_secure" type="string"  not-null="true"/>
       <property name="path_server" column="path_server" type="string"  not-null="false"/>
       <property name="path_www" column="path_www" type="string"  not-null="false"/>
       <property name="hosting_server_name" column="hosting_server_name" type="string"  not-null="false"/>
	
   </class>
</hibernate-mapping>


